<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <properties resource="generator.properties"></properties>
  <context id="mss" targetRuntime="MyBatis3">
    <commentGenerator>
      <property name="suppressAllComments" value="true"/>
    </commentGenerator>

    <jdbcConnection
      driverClass="${jdbc.driverClass}"
      connectionURL="${jdbc.connectionURL}"
      userId="${jdbc.userId}"
      password="${jdbc.password}">
    </jdbcConnection>
   <!-- <jdbcConnection
            driverClass="${mysql.jdbc.driverClass}"
            connectionURL="${mysql.jdbc.connectionURL}"
            userId="${mysql.jdbc.userId}"
            password="${mysql.jdbc.password}">
    </jdbcConnection>-->

    <javaTypeResolver>
      <property name="forceBigDecimals" value="false"/>
    </javaTypeResolver>

    <javaModelGenerator
      targetPackage="cn.com.huang"
      targetProject="src/main/java">
      <property name="enableSubPackages" value="true"/>
      <property name="trimStrings" value="true"/>
      <!--<property name="rootClass" value="BaseEntity" />-->
    </javaModelGenerator>

    <sqlMapGenerator
      targetPackage="mapper.hzh"
      targetProject="src/main/resources">
      <property name="enableSubPackages" value="true"/>
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER"
      targetPackage="cn.com.huang"
      targetProject="src/main/java">
      <property name="enableSubPackages" value="true"/>
    </javaClientGenerator>

  <!--
		<table tableName="TestByJerry" domainObjectName="Test" enableSelectByExample="false" enableDeleteByExample="false" enableCountByExample="false" enableUpdateByExample="false"/> -->

    <table tableName="PolicyProductsQuery" domainObjectName="PolicyProductsQuery" enableSelectByExample="false" enableDeleteByExample="false" enableCountByExample="false" enableUpdateByExample="false"/>


  </context>

</generatorConfiguration>